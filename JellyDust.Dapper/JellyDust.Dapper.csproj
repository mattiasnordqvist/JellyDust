<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIconUrl>https://raw.githubusercontent.com/mattiasnordqvist/JellyDust/master/icon.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/mattiasnordqvist/JellyDust.git</RepositoryUrl>
    <PackageProjectUrl>https://github.com/mattiasnordqvist/JellyDust</PackageProjectUrl>
    <Authors>mattiasnordqvist</Authors>
    <Company>mattiasnordqvist</Company>
    <AssemblyVersion>0.1.0.0</AssemblyVersion>
    <FileVersion>0.1.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="2.0.30" />
    <PackageReference Include="JellyDust" Version="1.0.0" />
    <PackageReference Include="System.Threading.Tasks" Version="4.3.0" />
  </ItemGroup>

</Project>
